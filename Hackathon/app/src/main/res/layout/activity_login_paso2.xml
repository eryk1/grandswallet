                style="@style/estilo_formularios_n2v2"/>
            <EditText
                android:id="@+id/et_homoclave_curp_beneficiario_remesas"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:inputType="textCapCharacters"
                style="@style/estilo_formularios_n2v2"
                android:minEms="2"
                android:maxEms="2"
                android:hint="XX"
                android:maxLength="2"
                android:layout_weight="1"/>
        </LinearLayout>
            <LinearLayout
