                    android:layout_height="wrap_content"
                    android:inputType="textCapWords"
                    android:textColor="@color/colorPrimary"
                    style="@style/estilo_formularios_n2v2"
                    android:layout_weight="4"/>
            </LinearLayout>
        <LinearLayout
